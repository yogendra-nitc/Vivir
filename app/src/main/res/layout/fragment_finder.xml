<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:context="com.example.yogendra.vivir.finder">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="200dp"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <Button
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:text="Change Password"
            android:id="@+id/changePassword"
            android:clickable="true"
            android:layout_marginBottom="5dp"
            />
        <Button
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:text="Complain List"
            android:id="@+id/complainList"
            android:clickable="true"
            android:layout_marginBottom="5dp"
            />
        <Button
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:text="User Dashboard"
            android:id="@+id/getDashboard"
            android:clickable="true"
            android:layout_marginBottom="5dp"
            />
        <Button
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:text="Delete Flat Record"
            android:id="@+id/deleteFlatRecord"
            android:clickable="true"
            android:layout_marginBottom="5dp"
            />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Flat Details"
            android:id="@+id/getFlatDetails"
            android:clickable="true"
            android:layout_marginBottom="5dp"
            />
        <Button
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:text="Notifications"
            android:id="@+id/notifications"
            android:clickable="true"
            android:layout_marginBottom="5dp"
            />
        <Button
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:text="Set Profile"
            android:id="@+id/setProfile"
            android:clickable="true"
            android:layout_marginBottom="5dp"
            />
        <Button
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:text="Reg User Search"
            android:id="@+id/reg_user_search"
            android:clickable="true"
            android:layout_marginBottom="5dp"
            />

    </LinearLayout>
    <LinearLayout
        android:layout_width="200dp"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:text="Request Details"
            android:id="@+id/requestDetails"
            android:clickable="true"
            android:layout_marginBottom="5dp"
            />
        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:text="Request List"
            android:id="@+id/requestList"
            android:clickable="true"
            android:layout_marginBottom="5dp"
            />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Update Flat Record"
            android:id="@+id/updateFlatRecord"
            android:clickable="true"
            android:layout_marginBottom="5dp"
            />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Upload Flat Details"
            android:id="@+id/upload_flat_details"
            android:clickable="true"
            android:layout_marginBottom="5dp"
            />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Upload Flat Image"
            android:id="@+id/upload_flat_images"
            android:clickable="true"
            android:layout_margin="5dp"
            />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Upload Rent Details"
            android:id="@+id/upload_rent_details"
            android:clickable="true"
            android:layout_marginBottom="5dp"
            />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Write Complain"
            android:id="@+id/writeComplain"
            android:clickable="true"
            android:layout_marginBottom="5dp"
            />

    </LinearLayout>
</LinearLayout>
